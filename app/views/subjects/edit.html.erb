<% @page_title = "Edit Subject" %>


<div class="row">
<div class="col-md-11">

    <h2><%= @page_title %></h2>

    <%= form_for(@subject, :html => {:class => "form-horizontal"}) do |f|%>

        <%= render(:partial => "form", :locals => {:f => f}) %>
        
        <div>
            <%= f.submit("Save", :class => "btn btn-primary") %>
            <%= link_to("Cancel", subjects_path, :class => "btn btn-default") %>
        </div>

    <% end %>

</div>
</div>